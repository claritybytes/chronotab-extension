{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\|/)(_|\\.)"
  },
  "opts": {
    "recurse": true,
    "readme": "README.md",
    "destination": "docs/reference/background.mdx"
  },
  "templates": {
    "default": {
      "outputFileExtension": ".mdx"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": false
  },
  "sourceType": "module"
}
